.row
  %img.col-xs-8.col-xs-offset-3.col-sm-offset-6.sizesub_medium{:src => "/assets/subleasy.png"}
  %h2.col-sm-4.col-xs-9.col-xs-offset-3.col-sm-offset-6 Log in
  .col-sm-4.col-xs-9.col-xs-offset-3.col-sm-offset-6
    = form_for(resource, as: resource_name, url: session_path(resource_name)) do |f|
      %div
        = f.label :email
        %br/
        = f.email_field :email, autofocus: true
      %div
        = f.label :password
        %br/
        = f.password_field :password, autocomplete: "off"
      - if devise_mapping.rememberable?
        %div
          = f.check_box :remember_me
          = f.label :remember_me
      %div= f.submit "Log in"
    = render "devise/shared/links"
